<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>@EXTENSION_CATEGORY@</category>
  <title>@EXTENSION_NAME@</title>
  <description>@EXTENSION_DESCRIPTION@</description>
  <version>@EXTENSION_MAJOR_VERSION@.@EXTENSION_MINOR_VERSION@.@EXTENSION_PATCH_VERSION@-@EXTENSION_STATUS@</version>
  <documentation-url>@EXTENSION_HOMEPAGE@</documentation-url>
  <license>@EXTENSION_LICENSE_SHORT_DESCRIPTION@</license>
  <contributor>@EXTENSION_CONTRIBUTORS@</contributor>
  <acknowledgements>@EXTENSION_ACKNOWLEDGEMENTS@</acknowledgements>
  <parameters>
    <label>Input volumes</label>
    <description>Input/output parameters</description>
    <image type="scalar">
      <name>preMRIFileName</name>
      <label>1st Grayscale-image</label>
      <channel>input</channel>
      <index>0</index>
      <description><![CDATA[Pre-op MRI]]></description>
    </image>
    <image type="label">
      <name>preEndoFileName</name>
      <label>1st Binary-image</label>
      <channel>input</channel>
      <index>1</index>
      <description><![CDATA[0/1 binary segmentation image of the 1st Grayscale-image]]></description>
    </image>
    <image type="scalar">
      <name>postMRIFileName</name>
      <label>2nd Grayscale-image</label>
      <channel>input</channel>
      <index>2</index>
      <description><![CDATA[2nd Grayscale-image, to be registered to the 1st Grayscale-image]]></description>
    </image>
    <image type="label">
      <name>postEndoFileName</name>
      <label>2nd Binary-image</label>
      <channel>input</channel>
      <index>3</index>
      <description><![CDATA[0/1 binary segmentation image of the 2nd Grayscale-image]]></description>
    </image>
  </parameters>
  <parameters advanced="true">
    <label>Advanced Parameters</label>
    <description><![CDATA[Parameters for localized deformable registration]]></description>
    <boolean>
      <name>deformableRegistrationLocally</name>
      <longflag>--deformableRegistrationLocally</longflag>
      <description><![CDATA[Only apply the deformable registration locally around the target. This saves running time significantly comparing to deformably registering the entire volumes.]]></description>
      <label>Deformable registration locally?</label>
      <default>true</default>
    </boolean>
  </parameters>
  <parameters>
    <label>Output volumes</label>
    <description><![CDATA[Filenames ofr output volumes]]></description>
    <image>
      <name>postToPreMRIFileName</name>
      <label>Registration Result</label>
      <channel>output</channel>
      <index>4</index>
      <description><![CDATA[Registered and warped 2nd Grayscale-image (onto 1st Grayscale-image)]]></description>
    </image>
  </parameters>


</executable>
